{
  "name": "bookmate-server",
  "version": "0.0.0",
  "private": true,
  "main": "main.js",
  "scripts": {
    "start": "nodemon ./bin/www",
    "server": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build-win",
    "build-mac": "electron-packager . --platform=\"darwin\" --out=\"build\" --overwrite --icon=\"logos/Icon.icns\"",
    "build-win": "electron-packager . --platform=\"win32\" --out=\"build\" --overwrite --icon=\"logos/BookMate.ico\"",
    "build-linux": "electron-packager . --platform=\"linux\" --out=\"build\" --overwrite --icon=\"logos/Icon.png\""
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "mongoose": "^5.12.9",
    "morgan": "~1.9.1",
    "multer": "^1.4.2"
  },
  "devDependencies": {
    "electron": "^12.0.7",
    "electron-packager": "^15.2.0",
    "nodemon": "^2.0.7"
  }
}
